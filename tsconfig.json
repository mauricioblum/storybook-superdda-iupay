{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": false,
    "baseUrl": "src",
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "rootDirs": ["src", "storybook/stories", "storybook/localComponents"]
  },
}
